use Data.String.Parser as Parser
use Apps.Kind.Parser as KParser
use Apps.Kind.SyntaxTree.Sugared as AST

//! Parses terms that start with an argument, like Pi and Lambda
// TODO: Will be more useful once they accept erased args, etc
KParser/term.arg_term : KParser/ AST/term {
  do Parser/ {
    
    KParser/term.pi_named // (var: term) -> term
    KParser/term.lambda_typed // (var: term) => term
  }
}