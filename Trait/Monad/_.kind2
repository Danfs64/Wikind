// Monad.new
//   (f    : Type -> Type)
//   (pure : (a: Type) (x: a) (f a))
//   (bind : (a: Type) (b: Type) (x: (f a)) (y: a -> (f b)) (f b))
// : Monad f
#derive[match]
record Monad (f: Type -> Type) {
  pure : ~(a: Type) -> (x: a) -> (f a)
  bind : ~(a: Type) -> ~(b: Type) -> (x: (f a)) -> (y: a -> (f b)) -> (f b)
}