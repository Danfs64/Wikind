List.unfoldr <a> <b> (f: a -> Maybe (Pair a b)) (x: a) : List b {
  List.unfoldr.go f x List.nil
}

List.unfoldr.go <a> <b> (f: a -> Maybe (Pair a b)) (x: a) (bs: List b) : List b {
  let res = (f x)
  match Maybe res {
    none => List.nil
    some value =>
      open Pair value
      List.unfoldr.go f value.fst (List.cons value.snd bs)
    }
}